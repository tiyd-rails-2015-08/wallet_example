<p id="notice"><%= notice %></p>

<h1>Listing Exchanges</h1>

<table>
  <thead>
    <tr>
      <th>Collector</th>
      <th>Amount</th>
      <th>Phone Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exchanges.each do |exchange| %>
      <%= render partial: "row", locals: {e: exchange} %>
    <% end %>
  </tbody>
</table>

<br>
